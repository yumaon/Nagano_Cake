<h1>配送先編集</h1>
<%= form_with model: @address do |f| %>
<div class="field">
    <%= f.label :postal_code, "郵便番号(ハイフンなし)" %>
    <%= f.text_field :postal_code, placeholder: "0000000" %>
  </div>

  <div class="field">
    <%= f.label :address, "住所" %>
    <%= f.text_field :address, placeholder: "東京都渋谷区代々木神園町0-0" %>
  </div>

  <div class="field">
    <%= f.label :name, "宛名" %>
    <%= f.text_field :name, placeholder: "令和道子" %>
  </div>

  <div class="actions">
    <%= f.submit "変更内容を保存" %>
  </div>
<% end %>