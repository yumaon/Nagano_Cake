<h1>会員情報編集</h1>

<%= form_with model: @customer, url: customers_information_path do |f| %>
  <label for="inputName">氏名</label>
  <%= f.text_field :last_name, id: "inputName" %>
  <%= f.text_field :first_name, id: "inputName" %>

  <label for="inputName_Kana">フリガナ</label>
  <%= f.text_field :last_name_kana, id: "inputName_Kana" %>
  <%= f.text_field :first_name_kana, id: "inputName_Kana" %>

  <label for="inputPostal_Code">郵便番号</label>
　<%= f.text_field :postal_code, id: "inputPostal_Code" %>

　<label for="inputAddress">住所</label>
　<%= f.text_field :address, id: "inputAddress" %>

　<label for="inputTelephone_Number">電話番号</label>
　<%= f.text_field :telephone_number, id: "inputTelephone_Number" %>

　<label for="inputEmail">メールアドレス</label>
　<%= f.text_field :email, id: "inputEmail" %>

　<%= f.submit "編集内容を保存" %>

<% end %>